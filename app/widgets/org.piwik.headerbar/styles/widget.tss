"#headerBar": {
    top: 0,
    height: '48dp',
    width: Ti.UI.FILL,
    backgroundImage: '/images/ab_solid_example.9.png',
    backgroundColor: '#CD1628'
}

"#backButton": {
    left: 0,
    width: '48dp',
    height: '48dp',
    backgroundColor: "transparent",
}

"#backangle": {
    left: '0dp',
    height: '40dp',
    width: '14dp',
}

"#homeIcon": {
    left: '14dp',
    height: '32dp',
    width: '32dp',
    touchEnabled: false,
    backgroundColor: 'transparent',
