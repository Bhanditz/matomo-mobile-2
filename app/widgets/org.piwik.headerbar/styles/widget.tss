}

"#headerTitle": {
    left: '53dp',
    color: '#ffffff',
    font: {
        fontSize: '18sp',
        fontFamily: 'Roboto-Thin'
    },
    height: Ti.UI.SIZE,
    width: Ti.UI.SIZE,
    wordWrap: false,
    ellipsize: true
}

"#actionButtons": {
    right: 0,
    width: Ti.UI.SIZE,
    height: '48dp',
    backgroundColor: 'transparent',
    layout: 'horizontal'
}