{
    "global": {
        "piwik": {
            "preferredMetrics": [
                "nb_hits",
                "entry_nb_visits",
                "exit_nb_visits",
                "nb_visits",
                "nb_pageviews",
                "nb_uniq_pageviews",
                "nb_actions",
                "nb_conversions",
                "revenue",
                "nb_events",
                "nb_impressions"
            ],
            "filterLimit": 30,
            "graph": {
                "fontSize": 9,
                "showMetricTitle": 0,
                "aliasedGraph": 1,
                "legendAppendMetric": 0,
                "backgroundColor": "efefef",
                "gridColor": "dcdcdc",
                "colors": "cb2026"
            }
        },
        "numDisplayedWebsitesInDashboard": 50,
        "caching": {
            "enabled": true
        },
        "tracking": {
            "enabled": true,
            "siteId": 48,
            "apiVersion": 1,
            "baseUrl": "http://mobileapp-beta.piwik.org",
            "piwikServerUrl": "http://demo.piwik.org/piwik.php",
            "maxTracksPerDay": 200
        },
        "faqUrl": "http://piwik.org/faq/mobile-app/",
        "appleAppStore": {
            "appId": "737216887"
        },
        "account": {
            "defaultReport": "MultiSites",
            "defaultReportDate": "yesterday"
        },
        "settings": {
            "locale": "en",
            "trackingEnabled": false,
            "preferEvolutionGraphs": true,
            "graphsEnabled": true,
            "httpTimeout": 60000,
            "validateSsl": true
        }
    },
    "adapters": ["localStorage", "properties", "sql", "piwikapi", "session"],
    "env:development": {},
    "env:test": {},
    "env:production": {},
    "os:ios": {},
    "os:android": {},
    "os:mobileweb": {
        "tracking": {
            "enabled": false,
            "siteId": 34,
            "apiVersion": 1,
            "baseUrl": "http://mobileapp.piwik.org",
            "piwikServerUrl": "http://demo.piwik.org/piwik.php",
            "maxTracksPerDay": 200
        }
    },
    "dependencies": {
        "org.piwik.tableviewrow": "1.0",
        "org.piwik.tableviewsection": "1.0",
        "org.piwik.imageview": "1.0",
        "org.piwik.headerbar": "1.0"
    }
}